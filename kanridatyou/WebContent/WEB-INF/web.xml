<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>kanridatyou</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>org.Login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>insert</servlet-name>
    <servlet-class>org.Kashidashi</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>insert_result</servlet-name>
    <servlet-class>org.Kashidashi_result</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>update</servlet-name>
    <servlet-class>org.update_Biz_source</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>update_result</servlet-name>
    <servlet-class>org.update_result</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login/check</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>insert</servlet-name>
    <url-pattern>/Kashidashi</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>insert_result</servlet-name>
    <url-pattern>/Kashidashi/result</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>update</servlet-name>
    <url-pattern>/update/conform</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>update_result</servlet-name>
    <url-pattern>/update/result</url-pattern>
  </servlet-mapping>
</web-app>